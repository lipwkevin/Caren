<div class='calendar'>
  <div class='date-info'>
    <div  id='calendar'>
    </div>
    <div class="input-group date" data-provide="datepicker">
      <input type="text" class="form-control datepicker" value="<%= @date %>">
      <div class="input-group-addon">
          <span class="glyphicon glyphicon-th"></span>
      </div>
    </div>
    <br>
    <div>
      <%=button_to 'Execute Schedule',run_schedule_path,class:"btn btn-default",method:"get"%>
      <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#my-Modal">
      Add
      </button>
    </div>
  </div>
  <div class='schedule container'>
    <!-- <div class='schedule-date'> -->
      <!-- <span> Date: <p class='current-date'> <%= Date.today.strftime("%m/%d/%Y") %></p></span> -->
    <!-- </div> -->
    <div class='schedule-detail'>
      <%= render 'schedule', {events:@events,date:@date} %>
    </div>
  </div>
</div>

<%= render "/events/form",{date:cookies[:date]}%>
